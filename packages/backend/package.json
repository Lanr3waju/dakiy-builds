{
  "name": "@dakiybuilds/backend",
  "version": "1.0.0",
  "private": true,
  "description": "DakiyBuilds backend API server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
